    <http://spinrdf.org/sp#text> """PREFIX : <http://www.metaphacts.com/resource/>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX kmi: <http://scholkg.kmi.open.ac.uk/aikg/ontology#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rules: <http://www.metaphacts.com/instances/qaRules#>
PREFIX cso: <http://cso.kmi.open.ac.uk/schema/cso#>
INSERT { GRAPH <http://scholkg.kmi.open.ac.uk/aikg/ontology#> {
 ?classShapeIri a sh:NodeShape ;
    sh:property [
      a sh:PropertyShape ;
      sh:path ?property ;
      sh:class ?range ;
  ] ;
    sh:targetClass ?targetClass .
  }}
WHERE {
  ?property rdfs:range ?range .
  ?property rdfs:domain ?union .
  ?union owl:unionOf ?listStart .
  ?listStart rdf:rest*/rdf:first ?targetClass .
  BIND ( IRI(CONCAT(STR(?targetClass), \"Shape\")) as ?classShapeIri)
}""";
